{
  "name": "xkeeper-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "AGPL-3.0-only",
  "author": "Wonderland",
  "contributors": [
    "0xArdy (https://github.com/0xArdy)",
    "turtlemoji (https://github.com/turtlemoji)"
  ],
  "scripts": {
    "dev": "yarn generate && vite",
    "build": "yarn generate && tsc && vite build",
    "generate": "wagmi generate",
    "preview": "vite preview",
    "test": "yarn cypress run",
    "test:dev": "yarn cypress open",
    "test:app": "VITE_TEST=true yarn dev",
    "test:fork": "eval $(grep '^VITE_RPC_URL' .env) && anvil --fork-url $VITE_RPC_URL --fork-block-number 10341674 --silent",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' 'cypress'",
    "lint:fix": "yarn run lint -- --fix",
    "prettier": "prettier src --check",
    "prettier:fix": "yarn run prettier -- --write",
    "format": "yarn run prettier:fix && yarn run lint:fix",
    "format:check": "yarn run prettier && yarn run lint",
    "prepare": "husky install && wonderland-crypto-husky-checks install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "src/**/*.{js,jsx,ts,tsx,css,scss,md,html}": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.14.8",
    "@mui/material": "5.14.8",
    "@rainbow-me/rainbowkit": "1.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.15.0",
    "viem": "1.19.15",
    "wagmi": "1.4.12"
  },
  "devDependencies": {
    "@defi-wonderland/crypto-husky-checks": "1.3.0",
    "@testing-library/jest-dom": "6.1.2",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/lodash.merge": "4.6.7",
    "@types/react": "18.2.24",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "@vitejs/plugin-react": "4.0.3",
    "@wagmi/cli": "1.5.1",
    "abitype": "0.9.8",
    "cypress": "13.6.0",
    "eslint": "8.45.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "lodash.merge": "4.6.2",
    "prettier": "3.0.3",
    "styled-components": "6.0.7",
    "typescript": "5.0.2",
    "vite": "5.0.10",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.34.3"
  }
}
